<script src="~/site/js/core.min.js"></script>
<script src="~/site/js/script.js"></script>

<script>
    function getContentDetail(categoryId) {

        $("#content").empty();
        var _url = 'GetContentDetail?categoryId=' + categoryId;
        $.ajax({
            type: "GET",
            url: _url,
            success: function (result) {
                $("#content").html(result);
            },
        });
    }

</script>